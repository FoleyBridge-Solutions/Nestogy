{
  "$schema": "https://opencode.ai/config.json",
  "mcp": {
    "fluxui-server": {
      "type": "local",
      "command": ["npx", "-y", "fluxui-mcp-server"],
      "enabled": true
    },
    "livewire-server": {
      "type": "local",
      "command": ["node", "/root/.augment/mcp-servers/livewire-mcp-server/build/index.js"],
      "enabled": true
    }
  }
}